[init]
	defaultBranch = main

[alias]
    drop = restore --
    dropall = restore -- .
    unadd = reset HEAD
    uncmt = reset HEAD^
    cma = commit --amend
    br = branch
    brd = branch -d
    ck = checkout
    ckb = checkout -b
    sw = switch
    swc = switch -c
    ri = rebase -i
	st = status -sb
	cmm = commit -m
    ll = log --oneline
    llg = log --oneline --grep
	pushom = push origin main
	pullom = pull origin main
[core]
	editor = vim
	autocrlf = input
